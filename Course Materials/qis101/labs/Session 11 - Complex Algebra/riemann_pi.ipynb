   "source": [
    "\"\"\"riemann_pi.ipynb\"\"\"\n",
    "# Cell 1\n",
    "\n",
    "from IPython.core.display import Math\n",
    "from sympy import primepi\n",
    "\n",
    "for n in range(1, 10):\n",
    "    # Use sympy to calculate the Riemann Pi for 10^n\n",
    "    riemann_pi = int(primepi(10**n))\n",
    "    display(Math(rf\"\\pi(10^{n}) = {riemann_pi:,}\"))"
   ]
  }
